	<PthreadsIncludes>$(SolutionDir)Vendor\pthreads\include</PthreadsIncludes>
	<PthreadsLibraries>$(SolutionDir)Vendor\pthreads\lib</PthreadsLibraries>
	<GenericIncludes>$(SolutionDir)Vendor\EXT\include</GenericIncludes>
