	</ItemGroup>
	<ItemGroup>
		<BuildMacro Include="PthreadsIncludes">
			<Value>$(PthreadsIncludes)</Value>
			<EnvironmentVariable>true</EnvironmentVariable>
		</BuildMacro>
		<BuildMacro Include="PthreadsLibraries">
			<Value>$(PthreadsLibraries)</Value>
