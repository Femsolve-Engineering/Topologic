		<BuildMacro Include="OpenCascadeIncludes">
			<Value>$(OpenCascadeIncludes)</Value>
			<EnvironmentVariable>true</EnvironmentVariable>
		</BuildMacro>
		<BuildMacro Include="OpenCascadeLibraries">
			<Value>$(OpenCascadeLibraries)</Value>
			<EnvironmentVariable>true</EnvironmentVariable>
		</BuildMacro>
